{
  "name": "@axa-ch/style-guide",
  "version": "0.15.5",
  "description": "AXA Switzerland Web Styleguide",
  "keywords": [
    "axa",
    "switzerland",
    "versicherungen",
    "web",
    "style",
    "guide"
  ],
  "main": "js/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/axa-ch/style-guide.git"
  },
  "scripts": {
    "dev": "better-npm-run dev",
    "build": "better-npm-run build",
    "start": "babel-node lib/server.js",
    "docs:build": "babel-node lib/build-docs.js",
    "bundles:watch": "babel-node webpack/webpack-dev-server.js",
    "bundles:build": "webpack --verbose --colors --display-error-details --config webpack/bundle.config.babel.js",
    "bundles:copy": "cp -R dist/bundles docs/dist/",
    "images:watch": "nodemon --watch images --exec npm run images:copy",
    "images:copy": "cp -R images docs/images2/",
    "less:watch": "nodemon --watch less --exec npm run less:copy",
    "less:copy": "cp -R less docs/less2/",
    "js:transpile": "babel js -d dist/js",
    "js:watch": "nodemon --watch js --exec npm run js:copy",
    "js:copy": "cp -R js docs/js2/",
    "all:watch": "concurrently --kill-others 'npm run images:watch' 'npm run less:watch' 'npm run js:watch' 'npm run bundles:watch'",
    "all:copy": "concurrently --kill-others 'npm run images:copy' 'npm run less:copy' 'npm run js:copy' 'npm run bundles:copy'"
  },
  "betterScripts": {
    "dev": {
      "command": "concurrently --kill-others 'npm run all:watch' 'sleep 2 && npm run docs:build'",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "npm run bundles:build && concurrently --kill-others 'npm run all:copy && npm run docs:build' 'npm run js:transpile'",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "AXA Versicherungen AG",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/axa-ch/style-guide/issues"
  },
  "browserify-shim": {
    "jquery": "global:jQuery",
    "baconjs": "global:Bacon",
    "iframe-resizer": "global:iFrameResize",
    "moment": "global:moment"
  },
  "homepage": "http://design.axa.ch",
