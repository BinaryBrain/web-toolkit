<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" >
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width">
    <meta name="description" content="{{package.description}}">
    <meta name="keywords" content="{{package.keywords}}">
    <meta name="author" content="{{package.author}}">

    <meta name="apple-mobile-web-app-title" content="{{package.title}}">
    <link rel="apple-touch-icon-precomposed" sizes="57x57" href="{{relative '/images/app-icon/ios/57x57.png'}}">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{relative '/images/app-icon/ios/72x72.png'}}">
    <link rel="apple-touch-icon-precomposed" sizes="76x76" href="{{relative '/images/app-icon/ios/76x76.png'}}">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{relative '/images/app-icon/ios/114x114.png'}}">
    <link rel="apple-touch-icon-precomposed" sizes="120x120" href="{{relative '/images/app-icon/ios/120x120.png'}}">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{relative '/images/app-icon/ios/144x144.png'}}">
    <link rel="apple-touch-icon-precomposed" sizes="152x152" href="{{relative '/images/app-icon/ios/152x152.png'}}">
    <link rel="apple-touch-icon-precomposed" sizes="180x180" href="{{relative '/images/app-icon/ios/180x180.png'}}">
    <link rel="apple-touch-startup-image" media="(device-width: 375px) and (device-height: 667px) and (-webkit-device-pixel-ratio: 2)" href="{{relative '/images/splash-screen/ios/750x1294.png'}}">

    <meta name="mobile-web-app-capable" content="yes">
    <meta name="application-name" content={{package.title}}>
    <link rel="manifest" href="{{relative '/manifest.json'}}">
    <link rel="icon" sizes="192x192" href="{{relative '/images/app-icon/android/192x192.png'}}">

    <meta name="msapplication-square70x70logo" content="{{relative '/images/app-icon/windows/128x128.png'}}">
    <meta name="msapplication-square150x150logo" content="{{relative '/images/app-icon/windows/270x270.png'}}">
    <meta name="msapplication-wide310x150logo" content="{{relative '/images/app-icon/windows/558x270.png'}}">
    <meta name="msapplication-square310x310logo" content="{{relative '/images/app-icon/windows/558x558.png'}}">

    <title>{{#if title}}{{title}} Â· {{/if}}{{package.title}}</title>

    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,600,700">

    {{#eq process.env.NODE_ENV 'production'}}
      <link rel="stylesheet" href="{{relative '/dist/docs.css'}}">
    {{/eq}}
  </head>
  <body class="push-lg-left" data-make="drawer" data-fade="drawer" data-target="body" data-in="overflow-hidden-md-down">
    <a href="#content" class="sr-only sr-only-focusable btn btn-default btn-block push-absolute-lg-left" id="skippy">
      <span class="skiplink-text">Skip to main content</span>
    </a>

    <a href="#" class="mask fade-out-delayed-md-down fade-out-lg-up" data-fade="drawer" data-target="body" data-in="fade-in-md-down" data-out="fade-out-delayed-md-down"></a>

    <header class="header header-fixed-top push-absolute-lg-left">
      <div class="header-main">
        <div class="container-fluid">
          <a href="#" class="header-burger float-xs-left hidden-lg-up" data-fade="drawer" data-target="body" data-in="is-open">
            <span class="header-burger-line"></span>
          </a>

          <div class="header-title float-xs-left">
            <span>{{title}}</span>
          </div>

          <div class="header-title float-xs-right">
            <a class="label label-primary" href="{{package.releasesUrl}}" >{{package.version}}</a>
          </div>
        </div>
      </div>
    </header>

    <main id="content" class="container-fluid docs-main">
      {{#> content }}
        {{{contents}}}
      {{/content}}
    </main>

    <div class="drawer togglable-xs-left persistent-lg-left" data-fade="drawer" data-target="body" data-in="in">
      <div class="header">
        <div class="header-main">
          <a href="{{relative '/'}}" class="header-brand" title="Go to home page">
            <img alt="AXA logo" src="{{relative '/images/axa.svg'}}" class="header-brand-image" width="46">
            <span style="padding-left: 15px;">
              <strong>AXA</strong>
              <br />
              <span>Web Toolkit</span>
            </span>
          </a>
        </div>
      </div>

      <div class="inline-search autocomplete">
        <input class="inline-search__input autocomplete__input" type='text' data-search="docs" data-search-index-data="{{relative '/searchIndex.json'}}" data-search-base-url="{{relative '/'}}" placeholder="Search pages...">
        <div class="inline-search__button">
          {{> icon name="search" classes="header-search__icon icon"}}
        </div>
      </div>

      <nav class="col-md-12 mobile-menu mobile-menu--collapsing" data-menu="collapsing">
        <ul class="mobile-menu__level" data-level>
          {{#with menu.side}}
            {{> menu}}
          {{/with}}
        </ul>
      </nav>

      <div class="col-md-12 mobile__separator"></div>

      <div class="col-md-12 list-group">
        <a class="list-group__item list-group__item--link" href="{{package.repository.url}}">
          {{> icon name="github" classes="list-group__icon"}}
          <span class="list-group__text">View on GitHub</span>
        </a>
        <a class="list-group__item list-group__item--link" href="{{package.releasesUrl}}">
          {{> icon name="empty" classes="list-group__icon"}}
          <span class="list-group__text">Releases</span>
        </a>
      </div>
    </div>

    <script>var __HREF = '{{relative link}}'</script>
    <script src="{{relative '/dist/docs.js'}}"></script>
    <script> window.intercomSettings = { app_id: 'i82zr1ej' }</script>
    <script>(function(){var w=window;var ic=w.Intercom;if(typeof ic==="function"){ic('reattach_activator');ic('update',intercomSettings);}else{var d=document;var i=function(){i.c(arguments)};i.q=[];i.c=function(args){i.q.push(args)};w.Intercom=i;function l(){var s=d.createElement('script');s.type='text/javascript';s.async=true;s.src='https://widget.intercom.io/widget/i82zr1ej';var x=d.getElementsByTagName('script')[0];x.parentNode.insertBefore(s,x);}if(w.attachEvent){w.attachEvent('onload',l);}else{w.addEventListener('load',l,false);}}})()</script>
  </body>
</html>
